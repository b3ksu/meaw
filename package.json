{
  "name": "meaw",
  "version": "5.0.0",
  "description": "Utilities for Unicode East Asian Width",
  "keywords": [
    "unicode",
    "east",
    "asian",
    "width"
  ],
  "repository": "https://github.com/susisu/meaw.git",
  "author": "Susisu <susisu2413@yahoo.co.jp>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "fetch": "ts-node -P tsconfig.scripts.json scripts/fetch.ts",
    "generate": "ts-node -P tsconfig.scripts.json scripts/generate.ts",
    "generate-test": "ts-node -P tsconfig.scripts.json scripts/generate.ts --test",
    "dump-eaw-version": "ts-node -P tsconfig.scripts.json scripts/dump-eaw-version.ts",
    "lint": "run-s lint:eslint lint:prettier",
    "lint-fix": "run-s lint-fix:eslint lint-fix:prettier",
    "lint:eslint": "eslint '*.js' src scripts",
    "lint-fix:eslint": "eslint --fix '*.js' src scripts",
    "lint:prettier": "prettier --check '*.js' src scripts",
    "lint-fix:prettier": "prettier --write '*.js' src scripts",
    "typecheck": "run-s typecheck:*",
    "typecheck:main": "tsc -p tsconfig.json --noEmit",
    "typecheck:scripts": "tsc -p tsconfig.scripts.json --noEmit",
    "test": "jest",
    "build": "rollup -c",
    "clean": "rimraf lib",
    "prepublishOnly": "run-s clean generate lint test build"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@susisu/eslint-config": "^0.0.39",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/node-fetch": "^2.5.7",
    "commander": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "jest": "^26.5.2",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.29.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
